module Enumerable
    def my_each
        return to_enum(:my_each) unless block_given?
        counter = 1
        while counter < arr.lenght
            yield(arr[counter])
            counter += 1
        end
    end

    def my_select
        return to_enum(:my_select) unless block_given?
        my_array = []
        my_each {|item| my_array << item if yield(item)}
        my_array
    end
end